    <script>


        $(document).ready(function () {
            $(".navbar-light .navbar-nav .nav-link").css("color", "#fff");
            $("nav.navbar .nav-link").mouseover(function () {
                $(".dropdown-hover:hover .dropdown-hover-content").slideDown(300);
                $(".header").css("background-color", "white");
                $(".navbar-light .navbar-nav .nav-link").css("color", "#333");
                $(".header.active").css("background-color", "#fff");
                $(".header.active .nav-link").css("color", "#333");
                // $(this).addClass("activeLink");
            });
            // $("nav.navbar .nav-item .nav-link").click(function(){
            //     $(this).addClass("activeLink").removeClass("removeLink");
            //     console.log("pathan")
            // });
            $(".dropdown-hover-content").mouseover(function () {
                // $("nav.navbar .nav-link").addClass("activeLink");
                $(this).siblings(".nav-link").addClass("activeLink")
                $(".header").css("background-color", "white");
                $(".navbar-light .navbar-nav .nav-link").css("color", "#333");
            });
            $("nav.navbar .nav-link").mouseleave(function () {
                $(".header").css("background-color", "transparent");
                $(".navbar-light .navbar-nav .nav-link").css("color", "#fff");
                $(".header.active header").css("background-color", "#fff");
                $(".header.active .nav-link").css("color", "#333 !important");
                // $(this).addClass("activeLink").removeClass("removeLink");
            });
            $(".dropdown-hover-content").mouseleave(function () {
                $(this).siblings(".nav-link").removeClass("activeLink")
                $(".header").css("background-color", "transparent");
                $(".navbar-light .navbar-nav .nav-link").css("color", "#fff");
            });
        });

        const aboutus = document.getElementById('about-us');
        if (window.location.pathname.includes('/about-us.html')) {
            aboutus.classList.add('activeLink')
        }
        const blogPage = document.getElementById('blog');
        if (window.location.pathname.includes('/blog.php')) {
            blogPage.classList.add('activeLink')
        }
    </script>

